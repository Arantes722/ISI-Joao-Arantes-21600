        <jsScript_script>// Limpeza do campo ID para conter apenas dígitos
var idLimpo = id.replace(/[^\d]/g, '');  // Remove tudo exceto números


// Validação do campo Date para o formato YYYY-MM-DD
var dateFormat = /^\d{4}-\d{2}-\d{2}$/;
var dateValido = dateFormat.test(date) ? date : null;  // Mantém a data se corresponder ao formato

// Limpeza da Description, removendo caracteres especiais e espaços extras
var descriptionLimpa = description.replace(/[^a-zA-Z0-9\s]/g, '');  // Remove caracteres especiais
descriptionLimpa = descriptionLimpa.replace(/\s+/g, ' ').trim();  // Remove espaços múltiplos e espaços no início/fim

// Adiciona os valores limpos e normalizados às variáveis de saída
outputId = idLimpo;
outputDate = dateValido;
outputDescription = descriptionLimpa;
</jsScript_script>
